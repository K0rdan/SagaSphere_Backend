{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "output",
    "logs"
  ],

  "verbose": true,
  "execMap": {
    "js": "npm run build && npm start"
  },
  "events": {
  },
  "watch": [
    "src/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}